generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Agenda {
  id             Int    @id @unique @default(autoincrement())
  Jour           String
  Matiere        String
  Nom_professeur String
  Salle          Int
}

model Grades {
    id          Int     @id @unique @default(autoincrement())
    Matiere     String
    Notation    String
    Note        Int
    Moyenne     Int
}
